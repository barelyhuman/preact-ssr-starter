{
  "compilerOptions": {
    // "jsxFactory": "h",
    "allowJs": true,
    // "checkJs": true,
    "jsx": "react-jsx",
    "module": "CommonJS",
    "target": "ESNext",
    // "jsxFragmentFactory": "Fragment",
    "jsxImportSource": "preact",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "skipLibCheck": true
  },
  "include": ["src/*"],
  "exclude": ["node_modules/**/*"]
}
